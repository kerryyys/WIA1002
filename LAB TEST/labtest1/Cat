
package lab.test.pkg1.pkgtry;

public class Cat extends Pet{
    private String name;
    
    Cat(String name,String breed,int weight,String sickness){
       super(breed,weight,sickness);
       this.name=name;
}

    public String getName(){
        return this.name;
    }
    public void setName(String name){
        this.name=name;
    }
}
