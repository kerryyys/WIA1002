public class bannerr {
    private boolean[][] banner;

    bannerr(boolean[][] banner) {
        this.banner = banner;
    }

    public void print() {
        for (int i = 0; i < banner.length; i++) {
            for (int j = 0; j < banner[i].length; j++)
                System.out.print(banner[i][j] ? "*" : " ");
            System.out.println();
        }
        System.out.println();
    }
}
